const int Inf           = (int)2e9 + 5;
const ll  Lnf           = (ll)2e18 + 5;
const int N             = 5e5 + 5;
const int NN            = 1e6 + 5;

ll ar[3];
ll I[3][3],T[3][3];


void mul(ll A[3][3], ll B[3][3], int dim){
    ll res[dim+1][dim+1];
    for(ll i=1;i<3;i++){
        for(ll j=1;j<3;j++){
            res[i][j] = 0;
            for(ll k=1;k<3;k++){
                ll x = (A[i][k]%M * B[k][j]%M)%M;
                res[i][j] = (res[i][j]%M+x%M)%M;
            }
        }
    }
    for(ll i=1;i<=dim;i++){
        for(ll j=1;j<=dim;j++){
            A[i][j] = res[i][j];
        }
    }
}

ll fib(ll n){
    //error(n);
    if(n<=2) return ar[n];
    for(ll i=1;i<3;i++){
        for(ll j=1;j<3;j++){
            if(i == j)I[i][j] = 1;
            else I[i][j] = 0;
            if(i == 1 and j == 1)T[i][j] = 0;
            else T[i][j] = 1;
        }
    }

 //   n--;

    while(n){
        if(n&1)mul(I,T,2);
        mul(T,T,2);
        n >>= 1LL;
    }

   // error(ar[1],I[1][1],ar[2],I[2][1]);
    return ((ar[1]%M*I[1][1]%M)%M + (ar[2]%M*I[2][1]%M)%M)%M;

}

int solve() 
{
    //happy coding
    ar[1] = ar[2] = 1;
    int t;
    cin>>t;
    while(t--){
        ll n;
        cin>>n;
        if(n == 0){
            cout<<0<<endl;
        }else if(n == 1){
            cout<<2<<endl;
        }
        else  cout<<fib(n+2)<<endl;
    }




    return 0;
    //error();
}
int main(){
    int test = 1;
    //readline(test);//(void)getchar();
    while (test--)solve();return 0;
}
